{
  "json.schemas": [
    {
      "fileMatch": [
        "vercel.json"
      ],
      "url": "https://openapi.vercel.sh/vercel.json"
    }
  ],
  "json.schemaDownload.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "files.associations": {
    "*.json": "jsonc"
  }
}
